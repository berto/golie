{
  "manifest_version": 2,

  "name": "Golie Visual",
  "description": "This extension provides a colorful visualizer for new tabs.",
  "browser_actions": {
    "default_icon": "golie-icon.png"
  },
  "version": "1.5",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
